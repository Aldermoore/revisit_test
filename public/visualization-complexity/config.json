{
    "$schema": "https://raw.githubusercontent.com/reVISit-studies/study/main/src/parser/StudyConfigSchema.json",
    "studyMetadata": {
        "title": "Visualization Complexity Study",
        "version": "pilot",
        "authors": [
            "CA3 @ Aarhus University"
        ],
        "date": "2024-07-25",
        "description": "A simple crowdsourcing study of the perceived complexity of data visualizations",
        "organizations": [
            "Aarhus University",
            "University of Utah",
            "WPI",
            "University of Toronto"
        ]
    },
    "uiConfig": {
        "contactEmail": "ellemose@cs.au.dk",
        "helpTextPath": "visualization-complexity/assets/help.md",
        "logoPath": "revisitAssets/revisitLogoSquare.svg",
        "withProgressBar": true,
        "autoDownloadStudy": false,
        "sidebar": false,
        "urlParticipantIdParam": "PROLIFIC_PID",
        "studyEndMsg": "**Thank you for completing the study. You may click this link and return to Prolific**: [https://app.prolific.com/submissions/complete?cc=C10I5YOL](https://app.prolific.com/submissions/complete?cc=C10I5YOL)"
    },
    "baseComponents": {
        "tagPickerWithStimuli": {
            "description": "A tag picker that allows for custom tags",
            "instruction": "You have to select or create at least one tag in order to continue.",
            "type": "react-component",
            "path": "visualization-complexity/assets/TagPickerWithStimuli.tsx",
            "parameters": {
                "stimulusNumber": 1
            },
            "response": [
                {
                    "id": "stimulus-complexity-score",
                    "prompt": "How complex does this data visualization seem to you?",
                    "required": true,
                    "hidden": false,
                    "location": "aboveStimulus",
                    "type": "likert",
                    "leftLabel": "Very simple",
                    "rightLabel": "Very complex",
                    "numItems": 10
                },
                {
                    "id": "tags",
                    "required": true,
                    "hidden": true,
                    "type": "iframe",
                    "prompt": "Choose the tags that best fit your reason for the complexity rating"
                }
            ]
        }
    },
    "components": {
        "introduction": {
            "type": "markdown",
            "path": "visualization-complexity/assets/introduction.md",
            "response": [
                {
                    "id": "prolificId",
                    "prompt": "Please enter your Prolific ID",
                    "required": true,
                    "location": "belowStimulus",
                    "type": "shortText",
                    "placeholder": "Prolific ID",
                    "paramCapture": "PROLIFIC_PID"
